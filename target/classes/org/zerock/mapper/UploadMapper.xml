<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.mapper.UploadMapper">
 	<insert id="create">
  		insert into attach values (#{uuid},#{uploadpath},#{filename},#{filetype},#{bno});
  	</insert>
  	<select id="findBno" resultType="org.zerock.domain.UploadVO">
  		select * from attach  where bno=#{bno}
  	</select>
  	<delete id="delete">
  		delete from attach  where uuid=#{uuid}
  	</delete>
  	<delete id="deleteAll">
  		delete from attach  where bno=#{bno}
  	</delete>
</mapper>